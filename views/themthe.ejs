                    <input type="number" name="idbankcard" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter pass">
                    <label for="">Tên ngân hàng</label>
                      <select class="form-control" name="bankname" id="sel1">
                        <option value="Vietcombank">Vietcombank</option>
                        <option value="OCB">OCB</option>
                        <option value="Techcombank">Techcombank</option>
                        <option value="Namabank">Nam á Bank</option>
                        
                      </select>
                  </div>
                 
                  <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            
              </div>
            </div>
          </div>
        </div> <!-- .section -->
      </div> <!-- .section -->


    <div class="map-wrap" id="map" data-aos="fade" style="background-color: white;"></div>


